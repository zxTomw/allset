{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".expo/**", "build/**"],
      "cache": true
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
